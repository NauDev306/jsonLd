{
	"name": "naudev306/jsonld",
	"description": "PHP Library to create JSON LD according to https://www.schema.org for SEO",
	"type": "library",
	"license": "Apache-2.0",
	"require": {
		"php": "^7.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.1.3"
	},
	"config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
	"autoload": {
	    "psr-4": {
	    	"NauDev\\JsonLd\\": "src/"
	    }
	},
	"autoload-dev": {
	    "psr-4": {
	    	"Tests\\": "tests/"
	    }
	}
}